             xmlns:local="clr-namespace:UventaApp"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Dictionarys/MainStyles.xaml"/>

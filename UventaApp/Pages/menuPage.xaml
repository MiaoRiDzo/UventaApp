<Page x:Class="UventaApp.Pages.menuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UventaApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="380" d:DesignWidth="760"
      Title="menuPage">

    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="userButton" Content="Пользователи и роли" Style="{DynamicResource btnStandart}" Click="userButton_Click"/>
        <Button x:Name="buildsButton" Content="Здания и объекты" Style="{DynamicResource btnStandart}" Click="buildsButton_Click"/>
        <Button x:Name="rentalsButton" Content="Договоры" Style="{DynamicResource btnStandart}" Click="rentalsButton_Click"/>
        <Button x:Name="tenantsButton" Content="Арендаторы" Style="{DynamicResource btnStandart}" Click="tenantsButton_Click"/>
    </StackPanel>
</Page>
